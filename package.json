{
    "name": "jarvis",
    "version": "3.0.0",
    "description": "Starter for HTML projects.",
    "engines": {
        "node": ">=6.9.1"
    },
    "scripts": {
        "start": "gulp --gulpfile config/gulp.js --shopify=true",
        "html": "gulp --gulpfile config/gulp.js --shopify=false",
        "watch": "npm run start",
        "gulp": "npm run start",
        "open": "shopify-themekit open",
        "configure": "shopify-themekit configure -d ./shopify/ --ignored-file config/settings_data.json",
        "download": "shopify-themekit download -d ./shopify/",
        "dev": "cross-env NODE_ENV=development npm run gulp -- dev",
        "build": "cross-env NODE_ENV=build npm run gulp -- build",
        "prod": "cross-env NODE_ENV=production npm run gulp -- build",
        "deploy": "cross-env NODE_ENV=build npm run start -- deploy",
        "deploy-prod": "cross-env NODE_ENV=production npm run start -- deploy",
        "purge": "npm run start -- purge",
        "clean": "npm run start -- purge"
    },
    "dependencies": {
        "@babel/polyfill": "^7.4.4",
